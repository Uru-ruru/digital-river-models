{
  "name": "uru/digital-river-models",
  "description": "Models for Digital River/ MyCommerce integrations",
  "license": "MIT",
  "keywords": [
    "digitalriver",
    "mycommerce"
  ],
  "homepage": "https://github.com/Uru-ruru/digital-river-models",
  "authors": [
    {
      "name": "alexanderpetrov",
      "email": "info@uru-ru.ru"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "friendsofphp/php-cs-fixer": "^3.50"
  },
  "autoload": {
    "psr-4": {
      "Uru\\DR\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Uru\\Tests\\DR\\": "tests/"
    }
  },
  "scripts": {
    "php-cs-fixer": "vendor/bin/php-cs-fixer fix --verbose --allow-risky=yes --using-cache=no --config .php-cs-fixer.php"
  }
}
